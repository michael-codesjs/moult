{
  "name": "@moult/experience-bff",
  "version": "0.9.0",
  "description": "bff service in the experience layer.",
  "main": "serverless.ts",
  "repository": "https://github.com/michael-codesjs/moult",
  "author": "Michael Phiri(github@michael-codesjs)",
  "license": "MIT",
  "scripts": {
    "prisma:generate": "prisma generate --schema=./../../schemas/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./../../schemas/prisma/schema.prisma",
    "prepare:layer": "./scripts/prepare-layer.sh",
    "build": "yarn prisma:generate && yarn prepare:layer && sls package",
    "predeploy": "yarn prisma:generate && yarn prepare:layer",
    "deploy": "sls deploy",
    "deploy:debug": "yarn pre:deploy && yarn prepare:layer && SLS_DEBUG=* sls deploy",
    "destroy": "sls remove",
    "generate-env": "ts-node scripts/generate-env.ts",
    "test": "jest --silent"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "serverless-appsync-plugin": "^2.9.1"
  },
  "devDependencies": {
    "@aws-sdk/client-eventbridge": "^3.279.0",
    "@aws-sdk/client-ssm": "^3.282.0",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@serverless/typescript": "^3.3.0",
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.25",
    "@types/ora": "^3.2.0",
    "aws-amplify": "^6.0.27",
    "babel-loader": "^9.1.3",
    "chalk": "2",
    "chance": "1.1.9",
    "esbuild": "^0.14.11",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "ora": "2",
    "prisma": "^5.11.0",
    "@prisma/client": "^5.22.0",
    "serverless": "^3.2.0",
    "serverless-esbuild": "^1.50.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "uuid": "9.0.0"
  }
}
